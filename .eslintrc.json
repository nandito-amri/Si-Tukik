{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "no-underscore-dangle": "off",
        "no-console": "off",
        "no-unused-vars": "off",
        "no-restricted-globals": "off",
        "no-alert": "off"
    }
}
